.nav-bar
  background-color white
  opacity 0.8
  display flex
  flex-direction column
  width 60px
  height 100vh
  justify-content space-around
  align-items center

  .nav-item
    color black

    a
      color black
      text-decoration none

    a:visited
      color inherit

  .nav-team-logo
    border-radius 100%
    height 50px
    width 50px
